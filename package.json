    "@types/mocha": "^10.0.10",
    "chai": "^5.1.2",
    "chai-as-promised": "^8.0.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "mocha": "^11.0.1",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typedoc": "^0.27.6",
    "typescript": "^5.7.3"
  },
